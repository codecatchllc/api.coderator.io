{
  "name": "api.coderator.io",
  "version": "0.0.0-semantic-release",
  "description": "API for Coderator",
  "main": "./src/server.ts",
  "author": "CodeCatch LLC <codecatchservice@gmail.com>",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/codecatchllc/api.coderator.io/blob/main/README.md",
  "repository": {
    "type": "git",
