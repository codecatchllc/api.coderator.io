  },
  "keywords": [
    "expressjs",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/codecatchllc/api.coderator.io/issues"
  },
  "scripts": {
    "start": "ts-node --files ./src/server.ts",
    "dev": "nodemon",
    "migrate": "prisma migrate dev --name init",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "pretty": "prettier --config .prettierrc src/**/*.ts --write",
    "test": "jest --roots test --verbose --watch",
    "test:ci": "jest --roots test --ci",
    "test:coverage": "jest --roots test --verbose --collect-coverage",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "argon2": "^0.28.5",
    "axios": "^0.27.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "helmet": "^5.0.2",
    "ioredis": "^5.0.6",
    "jsonschema": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
