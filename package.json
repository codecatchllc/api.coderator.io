    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^28.0.3",
    "lint-staged": "^12.4.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "prisma": "^3.13.0",
    "supertest": "^6.2.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.8.4"
  }
